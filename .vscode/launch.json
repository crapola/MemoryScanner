	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python : Current file",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal"
		},
		{
			"name": "Run test_program",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/test_program/bin/test_program.exe",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"miDebuggerPath": "F:\\MingW\\bin\\gdb.exe",
			"setupCommands": [
				{
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "SCons Build"
		}
	]
}